{
  "name": "wolke",
  "version": "0.4.1",
  "description": "Easy javascript deployment into the cloud",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastian-software/wolke"
  },
  "main": "lib/node.commonjs.js",
  "module": "lib/node.esmodules.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "rimraf bin && rimraf lib && prepublish",
    "release": "git push && release-it --github.release --npm.publish --non-interactive",
    "release:minor": "git push && release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "git push && release-it --github.release --npm.publish --non-interactive --increment major",
    "test": "jest"
  },
  "author": {
    "name": "Sebastian Fastner",
    "email": "s.fastner@sebastian-software.de",
    "url": "https://sebastian-software.de"
  },
  "bin": {
    "wolke": "bin/wolke"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": "eslint"
  },
  "keywords": [
    "aws",
    "lambda",
    "nodejs"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "jest": "^21.2.1",
    "prepublish": "^1.5.2",
    "readable-code": "^3.4.15",
    "release-it": "^5.0.0"
  },
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "aws-sdk": "^2.156.0",
    "base-x": "^3.0.2",
    "bluebird": "^3.5.1",
    "chalk": "^2.3.0",
    "cloudflare": "^2.2.0",
    "dotenv": "^4.0.0",
    "inquirer": "^4.0.0",
    "lodash": "^4.17.4",
    "meow": "^3.7.0",
    "nodent-runtime": "^3.0.4",
    "ora": "^1.3.0",
    "update-notifier": "^2.3.0",
    "which": "^1.3.0"
  }
}
